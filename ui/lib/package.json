{
  "name": "@eazyportal/document-store-service-ui-lib",
  "version": "0.0.1-SNAPSHOT",
  "description": "eazyPortal - File Upload Service UI Library",
  "author": "EazyPortal",
  "license": "https://github.com/eazyportal/core/blob/master/LICENSE",
  "proxy": "http://localhost:8080",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": "https://github.com/eazyportal/document-store-service-ui",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.jsx",
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "scripts": {
    "clean": "fse remove --quiet ./dist",
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "predeploy": "cd ../app && yarn build",
    "deploy": "gh-pages -d ../app/build"
  },
  "dependencies": {
    "@eazyportal/core-ui-lib": "^0.6.2"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@atao60/fse-cli": "*",
    "cross-env": "*",
    "gh-pages": "*",
    "microbundle-crl": "*",
    "npm-run-all": "*"
  },
  "files": [
    "dist"
  ]
}
